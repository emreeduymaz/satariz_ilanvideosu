{
  "name": "satarizilandetay",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:install": "playwright install --with-deps",
    "video:run": "node scripts/pipeline.mjs",
    "start:api": "node server.mjs",
    "docker:build": "docker build -t satarizilandetay .",
    "docker:test": "docker run --rm -e CI=1 -e LISTING_ID=$LISTING_ID -p 5173:5173 -v $(pwd)/test-results:/app/test-results satarizilandetay npx playwright test",
    "docker:video": "docker run --rm -e LISTING_ID=$LISTING_ID -e VARIANT=$VARIANT -v $(pwd)/satarizaevideo/outputs:/app/satarizaevideo/outputs satarizilandetay node scripts/pipeline.mjs",
    "docker:api": "docker run --rm -p 4000:4000 satarizilandetay npm run start:api"
  },
  "dependencies": {
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
